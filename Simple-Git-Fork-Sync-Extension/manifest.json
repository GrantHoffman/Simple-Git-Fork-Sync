{
	"name" : "Simple-Git-Fork-Sync",
	"description" : "Extension to update unmodfied master branch of your forked repositorys to current fork parents masters head",
	"version" :  "0.1",
	"manifest_version": 2,
	"background" : { 
		"scripts": ["simple-git-fork-sync.js"]
	},
	
	"icons": { "32": "icon.png" },
	
	"browser_action" : {
		"default_popup" : "popup.html",
		"default_icon" : "icon.png"
	},
	
	
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
	"permissions" : ["notifications", "https://ajax.googleapis.com/"]
}